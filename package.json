  "scripts": {
    "build": "tsc && npm run copy",
    "lint": "tslint -p tsconfig.json -c tslint.json src/**/*.ts",
    "copy": "node scripts/copy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhwang/node-red-contrib-ctc-beam-search.git"
  },
  "keywords": [
    "node-red",
    "beamsearch",
    "ctc"
  ],
  "node-red": {
    "nodes": {
      "tf-model": "dist/index.js"
    }
  },
  "author": "yh.wang@ibm.com",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/yhwang/node-red-contrib-ctc-beam-search/issues"
  },
  "homepage": "https://github.com/yhwang/node-red-contrib-ctc-beam-search#readme",
  "devDependencies": {
    "@types/node": "^12.12.17",
    "shelljs": "^0.8.3",
    "tslint": "^5.20.1",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "ctc-beam-search": "^0.1.0"
  }
}
